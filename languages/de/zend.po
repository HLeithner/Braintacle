#: Filter/Input.php:137
msgid "You must give a non-empty value for field '%field%'"
msgstr "Sie müssen einen nicht leeren Wert für Feld '%field%' angeben"

#: Validate/Date.php:47
msgid "'%value%' does not fit the date format '%format%'"
msgstr "'%value%' passt nicht zu Datumsformat '%pattern%'"

#: Validate/Db/Abstract.php:49
msgid "A record matching '%value%' was found"
msgstr "Ein Datensatz '%value%' ist bereits vorhanden"

#: Validate/Digits.php:52
msgid "'%value%' must contain only digits"
msgstr "'%value%' darf nur Ziffern enthalten"

#: Validate/EmailAddress.php
msgid "'%hostname%' is no valid hostname for email address '%value%'"
msgstr "'%hostname%' ist kein gültiger Hostname für Emailadresse '%value%'"

#: Validate/EmailAddress.php
msgid ""
"'%value%' is no valid email address in the basic format local-part@hostname"
msgstr "'%value%' ist keine gültige Emailadresse im Format local-part@hostname"

#: Validate/File/Size.php:49
msgid ""
"Maximum allowed size for file '%value%' is '%max%' but '%size%' detected"
msgstr ""
"Maximale erlaubte Größe für Datei '%value%' ist '%max%' aber '%size%' "
"übermittelt"

#: Validate/File/Upload.php:56
msgid "File '%value%' exceeds the defined ini size"
msgstr "Datei '%value%' überschreitet die definierte Größe."

#: Validate/File/Upload.php:59
msgid "File '%value%' was not uploaded"
msgstr "Datei '%value%' wurde nicht hochgeladen."

#: Validate/Hostname.php
msgid "'%value%' appears to be a DNS hostname but cannot extract TLD part"
msgstr ""
"'%value%' scheint ein DNS Hostname zu sein, aber TLD kann nicht extrahiert "
"werden"

#: Validate/Hostname.php
msgid ""
"'%value%' appears to be a local network name but local network names are not "
"allowed"
msgstr ""
"'%value%' scheint ein lokaler Netzwerkname zu sein, aber lokale "
"Netzwerknamen sind nicht erlaubt"

#: Validate/Hostname.php:66
msgid ""
"'%value%' appears to be a DNS hostname but cannot match TLD against known "
"list"
msgstr "'%value%' scheint ein DNS Hostname zu sein, TLD ist aber unbekannt"

#: Validate/Hostname.php:68
msgid ""
"'%value%' appears to be a DNS hostname but cannot match against hostname "
"schema for TLD '%tld%'"
msgstr ""
"'%value%' scheint ein DNS Hostname zu sein, passt aber nicht zu "
"Hostnamenschema für TLD '%tld'"

#: Validate/Hostname.php:70
msgid "'%value%' does not match the expected structure for a DNS hostname"
msgstr "'%value%' passt nicht zu erwarteter Struktur für einen DNS-Hostnamen"

#: Validate/Hostname.php:71
msgid "'%value%' does not appear to be a valid local network name"
msgstr "'%value%' scheint kein gültiger lokaler Netzwerkname zu sein"

#: Validate/Identical.php
msgid "'%value%' does not appear to be a valid IP address"
msgstr "'%value%' scheint keine gültige IP-Adresse zu sein"

#: Validate/Identical.php
msgid "The two given tokens do not match"
msgstr "Die beiden Werte sind verschieden"

#: Validate/NotEmpty.php:42
msgid "Value is required and can't be empty"
msgstr "Wert darf nicht leer sein."

#: Validate/Regex.php:46
msgid "'%value%' does not match against pattern '%pattern%'"
msgstr "'%value%' passt nicht zu Muster '%pattern%'"

#: Validate/StringLength.php
msgid "'%value%' is less than %min% characters long"
msgstr "'%value%' ist kürzer als %min% Zeichen"

#: Validate/StringLength.php:45
msgid "'%value%' is greater than %max% characters long"
msgstr "'%value%' ist länger als %max% Zeichen"

