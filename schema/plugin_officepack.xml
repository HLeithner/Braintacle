 <table>

  <name>officepack</name>

  <description>
    Details for installed MS office package, generated by MSofficeKey agent plugin:
    http://wiki.ocsinventory-ng.org/index.php/Plugins:MSofficeKey
  </description>

  <declaration>

   <field>
    <name>id</name>
    <type>integer</type>
    <length>4</length>
    <notnull>true</notnull>
    <autoincrement>1</autoincrement>
   </field>

   <field>
    <name>hardware_id</name>
    <type>integer</type>
    <length>4</length>
    <notnull>true</notnull>
   </field>

   <field>
    <name>officeversion</name>
    <type>text</type>
    <length>255</length>
   </field>

   <field>
    <name>product</name>
    <type>text</type>
    <length>255</length>
   </field>

   <field>
    <name>productid</name>
    <type>text</type>
    <length>255</length>
   </field>

   <field>
    <name>type</name>
    <type>text</type>
    <length>255</length>
   </field>

   <field>
    <name>officekey</name>
    <type>text</type>
    <length>255</length>
   </field>

   <field>
    <name>guid</name>
    <type>text</type>
    <length>255</length>
   </field>

   <field>
    <name>install</name>
    <type>integer</type>
    <length>4</length>
   </field>

   <field>
    <name>note</name>
    <type>text</type>
    <length>255</length>
   </field>

   <index>
    <name>officepack_primary</name>
    <primary>true</primary>
    <field>
     <name>id</name>
    </field>
   </index>

   <index>
    <name>officepack_hardware_id</name>
    <field>
     <name>hardware_id</name>
    </field>
   </index>

  </declaration>

 </table>
